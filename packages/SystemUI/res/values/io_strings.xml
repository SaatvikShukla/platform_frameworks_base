<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Battery level for expanded quick settings [CHAR LIMIT=2] -->
    <string name="battery_level_template"><xliff:g id="level" example="45">%d</xliff:g>%%</string>

    <!-- Quick settings -->
    <string name="quick_settings_brightness_label">Brightness</string>
    <string name="quick_settings_heads_up_label">Heads up</string>
    <string name="quick_settings_screen_off_label">Screen off</string>
    <string name="quick_settings_screenshot_label">Screenshot</string>
    <string name="quick_settings_volume_panel_label">Volume panel</string>

    <!-- Content description of the brightness tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_brightness">Volume panel.</string>

    <!-- Content description of the screen off tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_screen_off">Screen off.</string>

    <!-- Content description of the screenshot tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_screenshot">Screenshot.</string>

    <!-- Content description of the volume panel tile in quick settings (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_volume_panel">Volume panel.</string>

    <!-- Content description of the heads up tile in quick settings when off (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_off">Heads up off.</string>
    <!-- Content description of the heads up tile in quick settings when on (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_on">Heads up on.</string>

    <!-- Announcement made when heads up changes to off (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_changed_off">Heads up turned off.</string>
    <!-- Announcement made when heads up changes to on (not shown on the screen). [CHAR LIMIT=NONE] -->
    <string name="accessibility_quick_settings_heads_up_changed_on">Heads up turned on.</string>

    <!-- Strings for lockscreen shortcut hints -->
    <string name="left_shortcut_hint">Swipe right for %1$s</string>
    <string name="right_shortcut_hint">Swipe left for %1$s</string>

    <string name="lockscreen_message">Tap an icon on the left or right to reassign a lock screen shortcut.</string>
    <string name="lockscreen_default_target">Default</string>
    <string name="select_application">Select application</string>
    <string name="lockscreen_choose_action_title">Choose action</string>
    <string name="lockscreen_none_target">None</string>

</resources>
